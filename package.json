{
  "name": "ghumo-backend",
  "version": "1.0.0",
  "engines": {
    "node": "15.x"
  },
  "description": "This repo is about developing backend app support for already existing repo named ghumo. This backend is developed using Node JS, Express JS, Mongo DB etc.",
  "main": "server.js",
  "scripts": {
    "start-dev": "nodemon --exec babel-node server.js",
    "start": "npm run build && node ./build/server.js",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./build ./src -s",
    "clean": " rm -rf build && mkdir build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Anurag Thakur",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-env": "^2.4.1",
    "babel-preset-stage-0": "^6.24.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.2",
    "nodemon": "^2.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anurag9497/ghumo-backend.git"
  },
  "bugs": {
    "url": "https://github.com/Anurag9497/ghumo-backend/issues"
  },
  "homepage": "https://github.com/Anurag9497/ghumo-backend#readme"
}
